{
  "name": "turbotime",
  "displayName": "TurboTime",
  "description": "Turbo text time",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "turbotime.openConfig",
        "title": "TurboTime: Open Configuration"
      },
      {
        "command": "turbotime.openChat",
        "title": "TurboTime: Open Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "files": [
    "out/**/*",
    "media/**/*"
  ],
  "dependencies": {
    "axios": "^1.7.3",
    "dompurify": "^3.1.6",
    "ignore": "^5.3.1",
    "jsdom": "^24.1.1",
    "marked": "^14.0.0"
  }
}
